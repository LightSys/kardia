			:l:p_revision_id = 0 and
			:l:p_partner_key =* :p:p_partner_key and
			:c:p_country_code =* :l:p_country_code and
			:af:p_country_code =* :c:p_country_code and
			:af:p_address_set = 'STANDARD'
		    group by
			:p:p_partner_key
		    ";
	    use_having = yes;
	    }
	}

