						notimported = count(condition(:a_batch_number is null and :i_eg_status != 'returned', 1, null)),
						notimported_amt = isnull(sum(condition(:a_batch_number is null and :i_eg_status != 'returned', :i_eg_gift_amount, $0)), $0),
