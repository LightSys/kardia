				{
				sql = runserver("
					SELECT
						trx_date = substring(convert(string,:eg:i_eg_gift_trx_date),1,11),
						:eg:a_fund,
						cctext = isnull(:eg:a_fund + ' - ' + isnull(:c:a_fund_desc, '**INVALID**'), 'Needs QA: ' + :eg:i_eg_desig_name),
						:eg:i_eg_gift_amount,
