						ready = count(condition(:a_batch_number is null and :i_eg_donormap_confidence == 2 and :i_eg_fundmap_confidence == 2 and :i_eg_acctmap_confidence == 2 and :i_eg_status == 'paid', 1, null)),
						ready_amt = isnull(sum(condition(:a_batch_number is null and :i_eg_donormap_confidence == 2 and :i_eg_fundmap_confidence == 2 and :i_eg_acctmap_confidence == 2 and :i_eg_status == 'paid', :i_eg_gift_amount, $0)), $0),
						need_qa = count(condition(:a_batch_number is null and (isnull(:i_eg_donormap_confidence,0) < 2 or isnull(:i_eg_fundmap_confidence,0) < 2 or isnull(:i_eg_acctmap_confidence,0) < 2) and :i_eg_status == 'paid', 1, null)),
						need_qa_amt = isnull(sum(condition(:a_batch_number is null and (isnull(:i_eg_donormap_confidence,0) < 2 or isnull(:i_eg_fundmap_confidence,0) < 2 or isnull(:i_eg_acctmap_confidence,0) < 2) and :i_eg_status == 'paid', :i_eg_gift_amount, $0)), $0),
						pending = count(condition(:i_eg_status == 'pending' or :i_eg_status = 'cleared', 1, null)),
						pending_amt = isnull(sum(condition(:i_eg_status == 'pending' or :i_eg_status = 'cleared', :i_eg_gift_amount, $0)), $0),
						mostrecent = substring(convert(string, max(:i_eg_gift_trx_date)), 1, 11)
					from
						/apps/kardia/data/Kardia_DB/i_eg_gift_import/rows
					where
						:i_eg_gift_trx_date > dateadd(day, -90, getdate())
