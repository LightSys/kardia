					need_qa = (select count(1) from /apps/kardia/data/Kardia_DB/i_eg_gift_import/rows where :a_batch_number is null and (isnull(:i_eg_donormap_confidence,0) < 2 or isnull(:i_eg_fundmap_confidence,0) < 2 or isnull(:i_eg_acctmap_confidence,0) < 2) and :i_eg_status == 'paid' and dateadd(day, 90, :i_eg_gift_trx_date) > getdate() ),
					pending = (select count(1) from /apps/kardia/data/Kardia_DB/i_eg_gift_import/rows where :i_eg_status == 'pending' or :i_eg_status = 'cleared' and dateadd(day, 90, :i_eg_gift_trx_date) > getdate() ),
					mostrecent = (select substring(convert(string,max(:i_eg_gift_trx_date)),1,11) from /apps/kardia/data/Kardia_DB/i_eg_gift_import/rows ),
					notimpamount = (select sum(:i_eg_gift_amount) from /apps/kardia/data/Kardia_DB/i_eg_gift_import/rows where :a_batch_number is null and :i_eg_status != 'returned' and dateadd(day, 90, :i_eg_gift_trx_date) > getdate() )
