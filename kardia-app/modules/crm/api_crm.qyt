		activity_list "system/querytree"
		    {
		    sql = runserver("
			    DECLARE OBJECT activity
			    ;

			    SELECT
				:activity:cclist = isnull( (select sum(:cc:a_ledger_number + '|' + :cc:a_cost_center + ',') - ',' from /apps/kardia/data/Kardia_DB/a_cc_staff/rows cs, /apps/kardia/data/Kardia_DB/a_cost_center/rows cc where :cs:p_staff_partner_key = :s:p_partner_key and (:cc:a_bal_cost_center = :cs:a_cost_center or :cc:a_cost_center = :cs:a_cost_center) and :cc:a_ledger_number = :cs:a_ledger_number), '')
			    FROM
				/apps/kardia/data/Kardia_DB/p_staff/rows s,
				/apps/kardia/data/Kardia_DB/p_partner/rows p
			    WHERE
				:s:p_kardia_login = user_name() and
				:p:p_partner_key = :s:p_partner_key
			    LIMIT
				1
			    ;

			    SELECT
				:activity:group_id = :activity_group_id
			    FROM
				identity '/apps/kardia/modules/crm/timeline.qy?partner_key=" + escape(:eng_list:partner_id,"'","") + "&my_cclist='
			    ;

			    SELECT
				name = :e_activity_type + '|' + :e_reference_info,
				whom = :e_whom,
				initiated_by = condition(:e_initiation == 'C', 'Collaborator', 'Partner'),
				activity_date = :e_activity_date,
				activity_type = :e_activity_type,
				reference_info = :e_reference_info,
				info = :e_info
			    FROM
				/apps/kardia/data/Kardia_DB/e_activity/rows a
			    WHERE
				:a:e_activity_group_id = :activity:group_id
			    ORDER BY
				:a:e_activity_date desc
			    ");
		    use_having = yes;
		    }
		}

