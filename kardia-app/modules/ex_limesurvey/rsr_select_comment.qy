$Version=2$
myquery "system/query"
	{
	sql = "
        SELECT
            comment = eval(\":j:'\" + :cit:i_crm_field_comment + \"'\", \"O\")
        FROM
            IDENTITY
                WILDCARD ./testjson.json/responses/* j
                ,
                /apps/kardia/data/Kardia_DB/i_crm_import_type/rows cit
        WHERE
            :cit:i_crm_import_type_id = 1
	";
}